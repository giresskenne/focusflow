{
  "expo": {
    "name": "focusflow-app",
    "slug": "focusflow-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "com.anonymous.focusflow-app",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F3F6FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.focusflow-app",
      "splash": {
        "image": "./assets/splash-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#F3F6FF"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.focusflowapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications",
      ["./plugins/app-blocker-plugin", { "appGroup": "group.com.anonymous.focusflow" }]
    ]
  }
}
