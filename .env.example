# Supabase Configuration
# Copy this file to .env and replace with your actual Supabase project credentials
# For local dev with Supabase CLI, you may use http://127.0.0.1:54321
EXPO_PUBLIC_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=YOUR-ANON-KEY

# Development Settings
EXPO_PUBLIC_ENV=development

# Feature flags
# Gate cloud migration uploads during development; leave false until Phase 9
EXPO_PUBLIC_ENABLE_MIGRATION_UPLOAD=false
EXPO_PUBLIC_ENABLE_IOS_BLOCKING_DEV=false

# Optional local integration testing (NEVER commit real credentials)
# These enable the optional __tests__/supabase.int.test.js suite.
TEST_USER_EMAIL=
TEST_USER_PASSWORD=

# Local-only admin key for creating test users via scripts/createTestUser.js
# DO NOT COMMIT OR SHIP THIS; use only on localhost.
# SUPABASE_SERVICE_ROLE_KEY=